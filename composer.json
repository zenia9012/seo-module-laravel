{
    "name": "yevhenii/seo-module-laravel",
    "description": "Seo tools module. Seo helpers package for websites on Laravel and Lumen",
    "license": "MIT",
    "authors": [
        {
            "name": "Yevhenii Riabyi",
            "email": "zenia9012@gmail.com"
        }
    ],
    "homepage": "https://github.com/zenia9012",
    "keywords": [
        "laravel",
        "seo",
        "seo laravel",
        "seo admin",
        "laravel seo",
        "seo manager",
        "package",
        "tools",
        "helpers"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "composer-plugin",
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yevhenii\\Seo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yevhenii\\Seo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yevhenii\\Seo\\SeoServiceProvider"
            ],
            "aliases": {
                "Seo": "Yevhenii\\Seo\\Facades\\Seo"
            }
        }
    }
}
